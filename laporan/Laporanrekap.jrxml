	<queryString>
		<![CDATA[SELECT (SELECT SUM(harga_masing*jumlah) FROM transaksi WHERE status=1 AND tanggal >= $P{tanggal_dari} AND tanggal <= $P{tanggal_sampai}) AS penjualan,(SELECT SUM(harga) FROM pembelian WHERE tanggal >= $P{tanggal_dari} AND tanggal <= $P{tanggal_sampai}) AS pembelian,
